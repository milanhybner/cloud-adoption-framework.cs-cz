{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "src": "_bread",
        "dest": "bread",
        "files": [
          "**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/cloud-adoption-framework/_bread/toc.json",
      "bilingual_type": "hover over",
      "disable_contribution_on_translation_quality": ["MT"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/cloud-adoption-framework.cs-cz",
      "feedback_product_url": " https://azure-architecture.uservoice.com/forums/918625-architecture-guidance",
      "ms.service": "cloud-adoption-framework",
      "brand": "azure",
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "architecture-center",
          "folder_relative_path_in_docset": "./"
        }
      }
    },
    "fileMetadata": {},
    "template": [],
    "dest": "cloud-adoption-framework",
    "markdownEngineName": "markdig"
  }
}
